{
    "manifest_version": 3,
    "name": "Repeat Coder",
    "version": "1.0.0",
    "description": "Repeat repeat repeat.",
    "background": {
        "service_worker": "./js/background.js"
    },
    "content_scripts": [{
      "matches": ["https://atcoder.jp/contests/*/tasks/*"],
        "js": ["./js/content-script.js"]
    }],
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {}
    }